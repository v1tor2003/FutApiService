// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Team {
  id Int @id @default(autoincrement())
  name String
  shortName String?
  tla String?
  crest String?
  address String?
  website String?
  founded Int?
  clubColors String?
  venue String?
  lastUpdated DateTime?
}
